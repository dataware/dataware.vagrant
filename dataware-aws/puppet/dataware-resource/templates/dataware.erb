[DatawareResource]
port: 	       <%=port%>
resource_name: <%=resourcename%> 
resource_uri:  http://<%=@ipaddress_eth1-%>:<%=port%>
realm:         http://<%=@ipaddress_eth1-%>:<%=port%>

[DatawareResources]
<%= "resources: {" -%>
<% resources[0..-2].each do |resource| -%>
   <%= "\"" + resource + "\":{\"resource_uri\":\"http://" +  @ipaddress_eth1 +  ":" + port + "\"}," -%>
<% end -%>
<%= "\"" + resources.last + "\":{\"resource_uri\": \"http://" + @ipaddress_eth1 + ":" + port + "\"}" -%>
<%= "}" %>

[DatawareDB]
hostname: 127.0.0.1
username: <%=db_user%> 
password: <%=db_password%> 
dbname:   <%=dataware_db%>

[ResourceDB]
hostname: 127.0.0.1
username: <%=db_user%>   
password: <%=db_password%>   
dbname:   <%=resource_db%>

[Catalog]
catalog_uri: <%=catalog_uri%>
